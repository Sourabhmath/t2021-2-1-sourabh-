import java.util.*;
public class odd {
	public static void main(String[]args)
	{
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		if(a%2==0) 
		{
			for (int i = 1;i<(2*a) -1;i++)
			{
				if(i%2!=0)
				{
					System.out.print(i+" ");
				}
				}
		}
		else
		{
		for (int i = 1;i<(2*a )
				;i++)
		{
			if(i%2!=0)
			{
				System.out.print(i+" ");
			}
			
			}
		}		
}
}
